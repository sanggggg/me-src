{
  "private": true,
  "name": "me-src",
  "version": "1.0.0",
  "description": "me src",
  "workspaces": [
    "site",
    "plugin"
  ],
  "scripts": {
    "preinstall": "yarn workspace plugin build",
    "develop:site": "yarn workspace site develop",
    "debug:site": "yarn workspace site debug",
    "develop:plugin": "yarn workspace plugin develop",
    "develop:deps": "npm-run-all -l -p 'develop:api' 'develop:plugin'",
    "build:site": "yarn workspace site build",
    "build:plugin": "yarn workspace plugin build",
    "build": "npm-run-all -s 'build:plugin' 'build:site'",
    "serve:site": "yarn workspace site serve",
    "lint:site": "yarn workspace site lint",
    "lint:plugin": "yarn workspace plugin lint",
    "lint": "npm-run-all -s 'lint:plugin' 'lint:site'",
    "test": "yarn workspace plugin test",
    "clean": "npm-run-all -p 'clean:*'",
    "clean:site": "yarn workspace site clean"
  },
  "license": "MIT",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-graphql": "^4.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "gatsby-plugin-postcss": "^6.9.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.3"
  },
  "packageManager": "yarn@3.5.1"
}
